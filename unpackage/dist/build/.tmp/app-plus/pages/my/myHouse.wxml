<view class="tab-container bg-white"><block wx:for="{{rooms}}" wx:for-item="item" wx:for-index="key" wx:key="key"><block wx:if="{{rooms.length>0}}"><view class="tab-item border-bottom" data-item="{{item}}" hover-class="tab-item-hover" data-event-opts="{{[['tap',[['myHouseDetail',['$event']]]]]}}" catchtap="__e"><view><text class="iconfont iconaccount icon"></text><text class="tab-text">{{item.floorNum+"# "+item.unitNum+"单元"+item.roomNum}}</text></view><view class="tab-arrow"></view></view></block></block><block wx:if="{{rooms.length===0}}"><view class="tab-item border-bottom" hover-class="tab-item-hover" data-event-opts="{{[['tap',[['',['$event']]]]]}}" catchtap="__e"><view><text class="iconfont iconemail icon"></text><text class="tab-text">暂无房屋信息</text></view></view></block></view>